{
  "name_": "@lancelotlewis/v-g2",
  "name": "v-g2",
  "version": "1.1.1",
  "keywords": [
    "vue",
    "g2",
    "antv"
  ],
  "description": "A vue wrapper for antv-g2",
  "main": "dist/v-g4.ssr.js",
  "browser": "dist/v-g4.esm.js",
  "module": "dist/v-g4.esm.js",
  "unpkg": "dist/v-g4.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@antv/g2": "^4.0.0",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LancelotLewis/v-g2"
  },
  "publishConfig": {
    "registry_": "https://npm.pkg.github.com",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://lancelotlewis.github.io/v-g2/",
  "license": "MIT"
}
